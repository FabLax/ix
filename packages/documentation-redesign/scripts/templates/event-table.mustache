{{#events}}
#### {{ name }}
<table className="table-auto">
  <tbody>
    {{#name}}
    <tr>
      <td>Name</td>
      <td><code className="p-1">{{ name }}</code></td>
    </tr>
    {{/name}}
    <tr>
      <td>Event</td>
      <td><code className="p-1">{ `{{ event }}` }</code></td>
    </tr>
    {{#detail}}
    <tr>
      <td>Detail</td>
      <td><code className="p-1">{ `{{ detail }}` }</code></td>
    </tr>
    {{/detail}}
    <tr>
      <td>Description</td>
      <td>{ `{{ docs }}` }</td>
    </tr>
  </tbody>
</table>
{{/events}}

