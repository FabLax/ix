{{#props}}
#### {{ name }}
<table className="table-auto">
  <tbody>
    {{#attr}}
    <tr>
      <td>Attribute</td>
      <td><code className="p-1">{{ attr }}</code></td>
    </tr>
    {{/attr}}
    <tr>
      <td>Type</td>
      <td><code className="p-1">{ `{{{ type }}}` }</code></td>
    </tr>
    {{#default}}
    <tr>
      <td>Default</td>
      <td><code className="p-1">{ `{{{ default }}}` }</code></td>
    </tr>
    {{/default}}
    <tr>
      <td>Description</td>
      <td>{ `{{ docs }}` }</td>
    </tr>
  </tbody>
</table>
{{/props}}
