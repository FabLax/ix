{{#props}}
<ApiTable>
  <ApiTable.PropertyHeader name="{{name}}">
    {{#docsTags}}
      {{{ rTag }}}
    {{/docsTags}}
  </ApiTable.PropertyHeader>

  <ApiTable.Text name="Description">
    { `{{ docs }}` }
  </ApiTable.Text>

  <ApiTable.Code name="Attribute">
    {`{{{ attr }}}`}
  </ApiTable.Code>

  <ApiTable.Code name="Type">
   {`{{{ type }}}`}
  </ApiTable.Code>

  <ApiTable.Code name="Default">
    {`{{{ default }}}`}
  </ApiTable.Code>

</ApiTable>
{{/props}}
